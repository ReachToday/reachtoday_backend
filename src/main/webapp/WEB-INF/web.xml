<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>Reach Today</display-name>

	<servlet>
		<servlet-name>Mysqlconnector</servlet-name>
		<servlet-class>com.reachtoday.utils.Mysqlconnector</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>SendNumber</servlet-name>
		<servlet-class>com.reachtoday.actions.vechicle.SendNumber</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>ListBuses</servlet-name>
		<servlet-class>com.reachtoday.actions.vechicle.ListBuses</servlet-class>
	</servlet>


	<servlet>
		<servlet-name>getdata</servlet-name>
		<servlet-class>com.reachtoday.utils.getdata</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.reachtoday.actions.login.Loginuser</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>com.reachtoday.actions.login.Registeruser</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>searchuser</servlet-name>
		<servlet-class>com.reachtoday.actions.users.Searchuser</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>searchfriend</servlet-name>
		<servlet-class>com.reachtoday.actions.users.friend.Searchfriend</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>sendfriendrequest</servlet-name>
		<servlet-class>com.reachtoday.actions.users.friend.Sendfriendrequest</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>friendnotification</servlet-name>
		<servlet-class>com.reachtoday.actions.users.friend.friendnotification</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>nodejscall</servlet-name>
		<servlet-class>com.reachtoday.utils.NodeJsCall</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>TrackUser</servlet-name>
		<servlet-class>com.reachtoday.actions.users.tracking.UserTrack</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserLocationInsert</servlet-name>
		<servlet-class>com.reachtoday.actions.users.tracking.UserLocationAccess</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Mysqlconnector</servlet-name>
		<url-pattern>/Mysqlconnector</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>SendNumber</servlet-name>
		<url-pattern>/SendNumber</url-pattern>
	</servlet-mapping>



	<servlet-mapping>
		<servlet-name>ListBuses</servlet-name>
		<url-pattern>/ListBuses</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>getdata</servlet-name>
		<url-pattern>/getdata</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>searchuser</servlet-name>
		<url-pattern>/searchuser</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>searchfriend</servlet-name>
		<url-pattern>/searchfriend</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>sendfriendrequest</servlet-name>
		<url-pattern>/sendfriendrequest</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>friendnotification</servlet-name>
		<url-pattern>/friendnotification</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>nodejscall</servlet-name>
		<url-pattern>/nodejscall</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>TrackUser</servlet-name>
		<url-pattern>/TrackUser</url-pattern>
	</servlet-mapping>
		<servlet-mapping>
		<servlet-name>UserLocationInsert</servlet-name>
		<url-pattern>/UserLocation</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>


</web-app>
                         